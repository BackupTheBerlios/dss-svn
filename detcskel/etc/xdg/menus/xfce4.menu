<Menu>
  <Name>Applications</Name>
  <DefaultAppDirs />
  <DefaultDirectoryDirs />
  <KDELegacyDirs />
  <LegacyDir prefix="gnome-">/usr/share/gnome/apps</LegacyDir>
  <LegacyDir prefix="kappfinder-">/usr/share/apps/kappfinder/apps</LegacyDir>
 
  <Menu>
    <Name>Lostnfound</Name>
    <Directory>xfce4-lostnfound.directory</Directory>
    <OnlyUnallocated />
    <Include>
      <Not>
        <And>
          <Category>Core</Category>
          <Not><Category>KDE</Category></Not>
        </And>
        <Category>X-SuSE-YaST</Category>
      </Not>
    </Include>
  </Menu>
  <Menu>
    <Name>File Managers</Name>
    <Directory>xfce4-fm.directory</Directory>
