      <And>
      <Category>Network</Category>
      <Not><Category>X-KDE-More</Category></Not>
      </And>
    </Include>
    <Exclude>
      <Filename>Internet-pppoeconf.desktop</Filename>
    </Exclude>
    <Menu>
      <Name>More</Name>
      <Directory>kde-more.directory</Directory>
      <Include>
        <And>
        <Category>Network</Category>
        <Category>X-KDE-More</Category>
        </And>
      </Include>
    </Menu>
  </Menu>
  <Menu>
    <Name>Multimedia</Name>
    <Directory>xfce4-multimedia.directory</Directory>
    <Include>
      <And>
      <Category>AudioVideo</Category>
      <Not><Category>X-KDE-More</Category></Not>
      </And>
    </Include>
    <Exclude>
      <Filename>Multimedia-beep-media-player.desktop</Filename>
      <Filename>Multimedia-xine.desktop</Filename>
    </Exclude>
