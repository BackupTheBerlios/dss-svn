<?xml version="1.0" encoding="UTF-8"?> <!-- -*- SGML -*- -->

<voice version="0.1">
 
    <!--USB mouse: -->
    <!--$config.speach.add_mouseusb$ -->
    <match key="add_mouseusb" value="@content">
        <SABLE>
            <SPEAKER NAME="male3">
                <SAYAS MODE="acronym">USB</SAYAS> Mouse ready to use
            </SPEAKER> 
        </SABLE>
    </match>
    <!--$config.speach.rem_mouseusb$ -->
    <match key="rem_mouseusb" value="@content">
        <SABLE>
            <SPEAKER NAME="male3">
                <SAYAS MODE="acronym">USB</SAYAS> Mouse removed
            </SPEAKER> 
        </SABLE>
    </match>
